{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System.Net.Http.HttpClient": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Redis": {
    "ConnectionString": "override_me"
  },
  "Polygon": {
    "BaseAddress": "https://api.polygon.io/v2/",
    "ApiKey": "override_me"
  },
  "JobExecution": {
    "MaxJobsPerMinute": 5
  },
  "Scheduling": {
    "QuoteHistoryDays": 365,
    "Cron": {
      "DownloadTrackers": "0 1 * * *",
      "DownloadQuoteHistory": "30 1-23/6 * * *"
    }
  }
}
